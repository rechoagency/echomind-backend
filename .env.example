# EchoMind Backend - Environment Variables
# Copy this to .env and fill in the values

# Supabase Configuration
SUPABASE_URL=https://qgehxhkwnfxbdkbofhpm.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Reddit API Configuration
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# Google API Configuration (OAuth)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://echomind-backend-production.up.railway.app/api/google/callback

# N8N Configuration (optional)
N8N_API_KEY=your_n8n_api_key_here
